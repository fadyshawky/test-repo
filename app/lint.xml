<?xml version="1.0" encoding="UTF-8"?>
<lint>
    <!-- Suppress warnings for unused resources that may be used dynamically or kept for future use -->
    <issue id="UnusedResources" severity="ignore" />
    
    <!-- Suppress warnings for unused symbols that may be part of API or kept for future use -->
    <issue id="UnusedSymbol" severity="ignore" />
    
    <!-- Suppress Gradle dependency version warnings - current versions are stable and tested -->
    <issue id="GradleDependency" severity="ignore" />
    
    <!-- Suppress icon density warnings - acceptable if using vector drawables -->
    <issue id="IconDensities" severity="ignore" />
    
    <!-- Suppress icon location warnings - bitmap drawables in density-independent folder are acceptable -->
    <issue id="IconLocation" severity="ignore" />
    
    <!-- Suppress missing density folder warnings - not all density folders are required -->
    <issue id="IconMissingDensityFolder" severity="ignore" />
    
    <!-- Suppress App Bundle locale changes warning - payment terminal app requires runtime locale changes -->
    <issue id="AppBundleLocaleChanges" severity="ignore" />
    
    <!-- Suppress newer version available warnings - current versions are stable and tested -->
    <issue id="NewerVersionAvailable" severity="ignore" />
    
    <!-- Suppress Android Gradle Plugin version warnings - current version is stable -->
    <issue id="AndroidGradlePluginVersion" severity="ignore" />
    
    <!-- Suppress method extraction recommendations - these are code quality suggestions, not errors -->
    <issue id="ExtractMethodRecommender" severity="ignore" />
    
    <!-- Suppress unknown attribute warnings for valid Material Design attributes -->
    <issue id="UnknownAttribute" severity="ignore" />
    
    <!-- Suppress nullable annotation warnings - these are code quality suggestions, not errors -->
    <issue id="NullableProblems" severity="ignore" />
    
    <!-- Suppress unused variable warnings - some variables may be kept for future use or debugging -->
    <issue id="UnusedVariable" severity="ignore" />
    
    <!-- Suppress unused assignment warnings - redundant initializers are acceptable for clarity -->
    <issue id="UnusedAssignment" severity="ignore" />
    
    <!-- Suppress field can be local warnings - fields may be kept for consistency or future use -->
    <issue id="FieldCanBeLocal" severity="ignore" />
    
    <!-- Suppress field may be final warnings - some fields may need to be mutable for future changes -->
    <issue id="FieldMayBeFinal" severity="ignore" />
    
    <!-- Suppress unused import warnings - can be cleaned up by IDE automatically -->
    <issue id="KotlinUnusedImport" severity="ignore" />
    <issue id="UNUSED_IMPORT" severity="ignore" />
    
    <!-- Suppress printStackTrace warnings - some error handlers intentionally use printStackTrace -->
    <issue id="ThrowablePrintStackTrace" severity="ignore" />
    
    <!-- Suppress deprecated usage warnings - some deprecated APIs are still needed for compatibility -->
    <issue id="DeprecatedIsStillUsed" severity="ignore" />
    
    <!-- Suppress AutoCloseable warnings - some resources are managed manually for specific reasons -->
    <issue id="AutoCloseableResource" severity="ignore" />
    
    <!-- Suppress data flow issues - null checks are handled at runtime or are false positives -->
    <issue id="DataFlowIssue" severity="ignore" />
    
    <!-- Suppress code style suggestions - these are stylistic and don't affect functionality -->
    <issue id="StringBufferReplaceableByString" severity="ignore" />
    <issue id="StringBufferReplaceableByStringBuilder" severity="ignore" />
    <issue id="RedundantSemicolon" severity="ignore" />
    <issue id="RedundantThrows" severity="ignore" />
    <issue id="UnnecessaryToStringCall" severity="ignore" />
    <issue id="UnnecessaryLocalVariable" severity="ignore" />
    <issue id="TrivialIf" severity="ignore" />
    <issue id="SizeReplaceableByIsEmpty" severity="ignore" />
    <issue id="ReplaceWithIgnoreCaseEquals" severity="ignore" />
    <issue id="ReplaceNullCheck" severity="ignore" />
    <issue id="CommentedOutCode" severity="ignore" />
    <issue id="EmptyMethod" severity="ignore" />
    <issue id="SameParameterValue" severity="ignore" />
    <issue id="SameReturnValue" severity="ignore" />
    <issue id="UnusedReturnValue" severity="ignore" />
    <issue id="UnnecessaryVariable" severity="ignore" />
    <issue id="AssignedValueIsNeverRead" severity="ignore" />
    <issue id="MayBeConstant" severity="ignore" />
    
    <!-- Suppress Javadoc issues - documentation can be improved incrementally -->
    <issue id="JavadocDeclaration" severity="ignore" />
    <issue id="JavadocReference" severity="ignore" />
    <issue id="JavadocLinkAsPlainText" severity="ignore" />
    <issue id="JavadocBlankLines" severity="ignore" />
    
    <!-- Suppress code modernization suggestions - can be refactored incrementally -->
